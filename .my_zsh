# start tmux if not already in tmux
if [ -z "$TMUX" ]
then
	tmux
fi

# i luuuuuv vim
export VISUAL=nvim
export EDITOR="$VISUAL"

bindkey -e
# exports
PATH=$PATH:~/dev/tools
PATH=$PATH:~/dev/lsp
PATH=$PATH:~/.dotnet

# add prompt
source ~/.my_zsh_prompt

# aliases
source ~/.my_zsh_aliases

source ~/.my_zsh_gitconfig

# add ssh keys
eval "$(ssh-agent -s)" &> /dev/null

grep -slR "PRIVATE" ~/.ssh/ | xargs -I {} ssh-add -q {} 
