# start tmux if not already in tmux
if [ -z "$TMUX" ]
then
	tmux
fi

# i luuuuuv vim
export VISUAL=nvim
export EDITOR="$VISUAL"
export MANPAGER='nvim +Man!'
bindkey -e

# exports
source ~/.my_zsh_paths

# add prompt
source ~/.my_zsh_prompt

source ~/.my_zsh_aliases

source ~/.my_zsh_gitconfig

# add ssh keys
eval "$(ssh-agent -s)" &> /dev/null

grep -slR "PRIVATE" ~/.ssh/ | xargs -I {} ssh-add -q {} 

# completion for cli tools
autoload -U compinit
compinit -i
